[run]
branch = True
source =
    core
    scripts
omit =
    */tests/*
    */.venv/*
    */venv/*
    */site-packages/*
    */__pycache__/*
    */__init__.py

[report]
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    if TYPE_CHECKING:
    if __name__ == .__main__.:

[html]
directory = htmlcov
